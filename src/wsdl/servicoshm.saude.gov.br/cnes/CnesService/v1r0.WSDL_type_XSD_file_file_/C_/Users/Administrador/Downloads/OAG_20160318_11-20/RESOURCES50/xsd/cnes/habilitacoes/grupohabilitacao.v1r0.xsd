<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://servicos.saude.gov.br/schema/cnes/v1r0/grupohabilitacao" elementFormDefault="qualified" targetNamespace="http://servicos.saude.gov.br/schema/cnes/v1r0/grupohabilitacao">

	<xs:annotation>
		<xs:documentation><![CDATA[Este esquema representa Grupo da Habilitação do Estabelecimento de Saúde.]]></xs:documentation>
	</xs:annotation>
	
	<xs:element name="grupoHabilitacao" type="GrupoHabilitacaoType"/>

	<xs:complexType name="GrupoHabilitacaoType">
		<xs:sequence>
			<xs:element name="codigo" minOccurs="1" maxOccurs="1" type="xs:string">
				<xs:annotation>
					<xs:documentation><![CDATA[Código do Grupo da Habilitação do Estabelecimento de Saúde.]]>.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="descricao" minOccurs="1" maxOccurs="1" type="xs:string">
				<xs:annotation>
					<xs:documentation><![CDATA[Descrição Grupo da Habilitação do Estabelecimento de Saúde.]]>.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

</xs:schema>
